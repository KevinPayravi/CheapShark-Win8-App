                        <Button x:Name="buttonStoreSort" FontSize="15" Content="Store" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" Margin="3,0" Width="93" BorderThickness="0" Click="sortByStore_Click" BorderBrush="Transparent" />
