            <CheckBox x:Name="checkGameStop" Content="GameStop" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,407,136,-289" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkGetGames" Content="GetGames" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,511,136,-393" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkGamersGate" Content="GamersGate" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,329,136,-211" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkGameFly" Content="GameFly" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,433,136,-315" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkShinyLoot" Content="Shiny Loot" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,537,136,-419" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkGMG" Content="GreenMan" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,355,136,-237" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkGOG" Content="GOG" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,459,136,-341" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkHumble" Content="Humble" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,563,136,-445" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkAmazon" Content="Amazon" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,381,136,-263" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkOrigin" Content="Origin" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,485,136,-367" VerticalAlignment="Top" Width="150" />
            <CheckBox x:Name="checkDesura" Content="Desura" FontSize="15" IsChecked="True" HorizontalAlignment="Center" Height="22" Margin="36,589,136,-471" VerticalAlignment="Top" Width="150" />

            <Button x:Name="buttonCheckAll" Content="Check All" FontSize="13" HorizontalAlignment="Center" Height="38" Margin="36,613,170,-511" Background="Transparent" BorderBrush="#FFFFFF" VerticalAlignment="Top" Width="116" Click="buttonCheckAll_Click" />
